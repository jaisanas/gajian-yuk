<p id="notice"><%= notice %></p>

<h1>Disbursement Requests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @disbursement_requests.each do |disbursement_request| %>
      <tr>
        <td><%= link_to 'Show', disbursement_request %></td>
        <td><%= link_to 'Edit', edit_disbursement_request_path(disbursement_request) %></td>
        <td><%= link_to 'Destroy', disbursement_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Disbursement Request', new_disbursement_request_path %>
